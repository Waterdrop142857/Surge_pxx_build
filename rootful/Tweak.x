// 常见越狱路径
static NSArray *jailbreakPaths = @[
    @"/Applications/Cydia.app", @"/usr/bin/ssh", @"/var/lib/cydia", // 经典越狱路径
    @"/private/var/lib/dpkg/", @"/private/var/db/stash/", // iOS 15+ 有根越狱路径
